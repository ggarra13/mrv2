<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleExecutable</key>
	<string>@INSTALL_NAME@</string>
	<key>CFBundleIdentifier</key>
	<string>com.@INSTALL_NAME@.@INSTALL_NAME@</string>
	<key>CFBundleName</key>
	<string>@INSTALL_NAME@</string>
	<key>CFBundleIconFile</key>
	<string>@INSTALL_NAME@.icns</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>@mrv2_VERSION@</string>
	<key>CFBundleVersion</key>
	<string>@mrv2_VERSION@</string>
	<key>NSHumanReadableCopyright</key>
	<string>Copyright (c) 2022-Present Film Aura, LLC.</string>
	<key>NSHighResolutionCapable</key>
	<true/>

    <key>UTExportedTypeDeclarations</key>
    <array>
        <dict>
            <key>UTTypeIdentifier</key>
            <string>com.mrv2.mrv2.session</string>
            <key>UTTypeDescription</key>
            <string>mrv2 Session File</string>
            <key>UTTypeConformsTo</key>
            <array>
                <string>public.data</string>
            </array>
            <key>UTTypeTagSpecification</key>
            <dict>
                <key>public.filename-extension</key>
                <array>
                    <string>mrv2s</string>
                </array>
            </dict>
        </dict>
    </array>

    <key>CFBundleDocumentTypes</key>
    <array>
        <dict>
          <key>CFBundleTypeRole</key>
          <string>Viewer</string>
            <key>LSItemContentTypes</key>
            <array>
                <string>public.camera-raw-image</string>
                <string>com.adobe.digital-negative</string>
                <string>com.adobe.photoshop-image</string>
            </array>
        </dict>

        <dict>
	  <key>CFBundleTypeName</key>
	  <string>Image Files</string>
          <key>CFBundleTypeRole</key>
          <string>Viewer</string>
            <key>LSItemContentTypes</key>
            <array>
                <string>com.openexr.image</string>
                <string>public.dpx</string>
                <string>public.jpeg</string>
                <string>public.png</string>
                <string>public.tiff</string>
                <string>public.tga</string>
                <string>public.bmp</string>
                <string>com.sgi.sgi-image</string>
                <string>org.opensfx.otio</string>
            </array>
        </dict>

        <dict>
          <key>CFBundleTypeRole</key>
          <string>Viewer</string>
            <key>LSItemContentTypes</key>
            <array>
                <string>public.movie</string>
                <string>public.video</string>
                <string>public.mpeg-4</string>
                <string>com.apple.quicktime-movie</string>
                <string>public.avi</string>
                <string>public.mpeg</string>
            </array>
        </dict>

        <dict>
          <key>CFBundleTypeName</key>
          <string>mrv2 Session File</string>
          <key>CFBundleTypeRole</key>
          <string>Editor</string>
            <key>LSItemContentTypes</key>
            <array>
                <string>com.mrv2.mrv2.session</string>
            </array>
        </dict>
    </array>
</dict>
</plist>
